    //font-family georgia, serif
    font-weight 400
    // line-height 1.4
    font-size 14px
    margin 0 auto
    top 0
    // background-color #ededed
    background-color var(--menu-highlight-color)
    -webkit-font-smoothing antialiased
    overflow hidden
    // overflow-y hidden

