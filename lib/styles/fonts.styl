@font-face
    font-family 'latolight'
    font-display auto
    src url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-light-webfont.eot')
    src url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-light-webfont.eot?#iefix') format('embedded-opentype'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-light-webfont.woff2') format('woff2'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-light-webfont.woff') format('woff'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-light-webfont.ttf') format('truetype'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-light-webfont.svg#latolight') format('svg')
        // https://cdn.jsdelivr.net/font-lato/2.0/LatoLatin/LatoLatin-Regular.css
    font-weight normal
    font-style normal

@font-face
    font-family 'latoregular'
    font-display auto
    src url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-regular-webfont.eot')
    src url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-regular-webfont.eot?#iefix') format('embedded-opentype'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-regular-webfont.woff2') format('woff2'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-regular-webfont.woff') format('woff'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-regular-webfont.ttf') format('truetype'),
        url('https://desarrolloactivo.ams3.cdn.digitaloceanspaces.com/fonts/lato/lato-regular-webfont.svg#latoregular') format('svg')
    font-weight normal
    font-style normal

